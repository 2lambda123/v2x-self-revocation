            case Init
            solve( !KU( sign(<prl, ('1'+t_rev+z)>, ~ltk) ) @ #vk.4 )
              case RA_generate_heartbeat_case_1
              solve( (¬(#k < #vr.2))  ∥ (∀ z. (('1'+'1') = ('1'+z)) ⇒ ⊥) )
                case case_1
                by contradiction /* from formulas */
              next
                case case_2
                by contradiction /* from formulas */
