lemma processing_hb_possible [heuristic=o "oracle.py"]:
  exists-trace "∃ hb #i. HeartbeatProcessed( hb ) @ #i"
/*
guarded formula characterizing all satisfying traces:
"∃ hb #i. (HeartbeatProcessed( hb ) @ #i)"
*/
simplify
solve( HeartbeatProcessed( hb ) @ #i )
  case TC_process_heartbeat_no_revoke
  solve( !Pseudonym( ps ) ▶₂ #i )
    case Init_case_1
    solve( !Pk( 'RA', pk(x) ) ▶₃ #i )
      case Init
      solve( !Parameters( tv ) ▶₁ #i )
        case Init
        solve( !KU( sign(<prl, t_hb>, ~ltk) ) @ #vk.4 )
          case RA_generate_heartbeat_case_1
          solve( (('1'+'1') = t)  ∥ (∃ z. (('1'+'1') = (t+z))) )
            case case_1
            solve( !Time( 'TC', ('1'+'1') ) ▶₀ #i )
              case TC_process_heartbeat_no_revoke
              solve( !KU( sign(<prl, ('1'+'1')>, ~ltk) ) @ #vk.9 )
                case RA_generate_heartbeat_case_1
                solve( (('1'+t) = ('1'+'1'))  ∥ (∃ z. (('1'+t) = ('1'+'1'+z))) )
                  case case_1
                  solve( (¬(#vr.7 < #vr.2))  ∥ (∀ z. (('1'+'1') = ('1'+z)) ⇒ ⊥) )
                    case case_1
                    solve( !Time( 'TC', '1' ) ▶₀ #vr.4 )
                      case Init
                      solve( !Time( 'RA', '1' ) ▶₀ #vr.7 )
                        case Init
                        SOLVED // trace found
                      qed
                    qed
                  qed
                qed
              qed
            qed
          qed
        qed
      qed
    qed
  qed
qed

