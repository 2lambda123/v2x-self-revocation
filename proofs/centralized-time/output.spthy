lemma sign_possible [heuristic=I]:
  exists-trace "∃ msg ps #i. Signed( msg, ps ) @ #i"
/*
guarded formula characterizing all satisfying traces:
"∃ msg ps #i. (Signed( msg, ps ) @ #i)"
*/
simplify
solve( !Time( 'TC', t ) ▶₀ #i )
  case Init_case_1
  solve( !Pseudonym( ps ) ▶₁ #i )
    case Init
    solve( !Ltk( pk(~ps_key), ps_key.1 ) ▶₂ #i )
      case Init
      SOLVED // trace found
    qed
  qed
qed

